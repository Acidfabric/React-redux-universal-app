{
  "name": "bnbservices",
  "version": "1.0.0",
  "description": "AWS Simple email service",
  "main": "index.js",
  "scripts": {
    "start": "cross-env BABEL_DISABLE_CACHE=1 NODE_ENV=development nodemon index.js",
    "start:prod": "cross-env NODE_ENV=production node index.js",
    "bs": "npm run clean && npm run build && npm run build:server && npm run start:prod",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "build:server": "cross-env NODE_ENV=production webpack --config webpack.config.server.js",
    "test": "mocha server/test --require babel-polyfill --compilers js:babel-register",
    "clean": "rimraf server/uploads/* server/uploads/private_key.pem",
    "slate": "rimraf node_modules && npm install"
  },
